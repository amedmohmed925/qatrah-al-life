{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/src/server.js" },
    { "src": "/api-docs/(.*)", "dest": "/src/server.js" },
    { "src": "/api-docs", "dest": "/src/server.js" }
  ]
}
